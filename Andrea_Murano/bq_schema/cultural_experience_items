-- Books table
CREATE TABLE books (
  ingest_ts TIMESTAMP,
  book_id STRING,
  title STRING,
  author_name STRING,
  first_publish_year INT64,
  language STRING,
  book_url STRING
);

-- Artworks table
CREATE TABLE artworks (
  ingest_ts TIMESTAMP,
  book_id STRING,
  object_id STRING,
  title STRING,
  artist_name STRING,
  medium STRING,
  object_date STRING,
  object_url STRING,
  image_url STRING
);

-- Music Tracks table
CREATE TABLE music_tracks (
  ingest_ts TIMESTAMP,
  book_id STRING,
  track_id STRING,
  title STRING,
  artist STRING,
  album STRING,
  release_date STRING,
  preview_url STRING
);
