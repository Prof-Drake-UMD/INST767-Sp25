import requests
import json
    
url = "https://api.weather.gov/alerts"
    
params = {
    'area': "MD",
    'limit': 3,
    'severity': "Severe"
    }

response1 = requests.get(url, params=params, )

#retrieving json

if response1.status_code == 200:
    data1 = response1.json()
    print(data1)
else:
    print(f"Error: {response1.status_code}")

